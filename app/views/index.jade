html(lang='en', ng-app='app')
  head
    meta(charset='utf-8')
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name='viewport', content='width=device-width', initial-scale='1.0')
    meta(name='description', content='')
    meta(name='author', content='')
    title(ng-bind-template='{{pageTitle}}')
    script.
      window.brunch = window.brunch || {};
      window.brunch['auto-reload'] = {
        enabled: true
      };
      window.root = {}
      window.root.env = 'dev'
    != css('styles/vendor')
    != css('app-styles')
    != js('js/vendor')
    != js('app')
    link(href='/css/font-awsome.css', rel='stylesheet', type='text/css')
    script(src="http://localhost:35729/livereload.js")
  body(ng-app='app')
    .container
      nav.navbar-default(role='navigation')
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') Finance
        .collapse.navbar-collapse
          ul.nav.navbar-nav
            li.active
              a(href='/line_items') Line Items
            li
              a(href='#about') About
            li
              a(href='#contact') Contact

      .container.main-content
        ng-view
    footer.footer
      .container
        p
          small
            a(href='https://github.com/scotch/angular-brunch-seed') angular-brunch-seed | source