ng-include(src="'/partials/common/finance_header.html'")
.accounts_index.container
  .new.row
    a.btn.btn-primary.col-xs-12.add_transaction(ng-href='/accounts/new') Add Account

  .main_list.list-group
    .list-group-item.row(ng-repeat='item in accounts')
      .col-xs-12.col-md-10
        a(ng-href='/accounts/{{item.id}}')
          {{item.name}}
      .hidden-xs.col-md-2.text-right
        a.btn.btn-default.btn-sm.edit(ng-href='/misc/import?accountId={{item.id}}') Import   