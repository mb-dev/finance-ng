ng-include(src="'/partials/common/finance_header.html'")
.accounts_show.container.show_page
  .panel.panel-default.row
    .panel-heading
      .btns-parent.pull-right
        a.btn.btn-default.btn-sm.edit(ng-href='/accounts/{{item.id}}/edit?returnto=accounts/{{item.id}}') Edit  
        a.btn.btn-default.btn-sm.delete(ng-href='/accounts/{{item.id}}/delete') Delete
      h4.panel-title 
        |Account Details
    .panel-body.as-prop-columns.col-xs-12
      .row
        .left-pane Name
        .right-pane {{item.name}}
      .row
        .left-pane Import Format
        .right-pane {{item.importFormat}}